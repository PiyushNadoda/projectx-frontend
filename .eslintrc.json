{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "quotes": ["error", "single"],
    "camelcase": "error",
    "no-confusing-arrow": "error",
    "no-empty": "error",
    "no-empty-function": "error",
    "eqeqeq": "warn",
    "curly": "error"
  }
}
